
pipeline {
    agent any
    environment {
        ENV_URL = "pipeline.google.com"
    }
    stages {
        stage("First Stage") {
            steps {
                sh "echo Hello world"
                sh "echo ${ENV_URL}"
            }
        }
        stage("Second Stage") {
            steps {
                echo "Welcome world! We will do CICD tomorrow"
            }
        }
    }

}


